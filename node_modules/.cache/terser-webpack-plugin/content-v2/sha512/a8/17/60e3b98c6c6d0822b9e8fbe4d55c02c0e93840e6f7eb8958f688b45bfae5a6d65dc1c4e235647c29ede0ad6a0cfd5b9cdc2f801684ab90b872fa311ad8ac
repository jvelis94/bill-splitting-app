';\n\n\nconst IndividualItem = (props) => {\n    const item = props.item\n\n    const incrementQty = (name, personId) => {\n        props.incrementItemQuantity(item.name, props.personId)\n    }\n\n    const decrementQty = () => {\n        props.decrementItemQuantity(item.name, props.personId)\n    }\n\n    return (\n        <div className={styles.itemFlex}>\n            <h4>{item.name}</h4>\n            <h4 className={styles.centerMoneyQuantity}>\n                <div className={styles.centerActionItems}>\n                    <RemoveIcon onClick={decrementQty} color={item.qty === 1 ? 'disabled' : 'inherit'} />\n                    {item.qty}\n                    <AddIcon onClick={incrementQty}/>\n                </div>\n            </h4>\n            <h4 className={styles.centerMoneyQuantity}>{item.price}</h4>\n        </div>\n    )\n}\n\nexport default IndividualItem","import { useState, useRef } from 'react'\nimport styles from './ItemForm.module.css'\n\nconst ItemForm = (props) => {\n    const nameRef = useRef()\n    const priceRef = useRef()\n\n    const handleFormSubmit = (event) => {\n        event.preventDefault()\n        props.addItemToPerson(nameRef.current.value, priceRef.current.value, props.personId)\n        nameRef.current.value = \"\"\n        priceRef.current.value = \"\"\n    }\n\n    return (\n        <form className={styles.formContainer} onSubmit={handleFormSubmit}>\n            <input type=\"text\" name=\"name\" placeholder=\"burger\" className={styles.formInputs} ref={nameRef} />    \n            <input type=\"text\" name=\"price\" placeholder=\"20\" className={styles.formInputs} ref={priceRef} />\n            <input type=\"submit\" style={{display: 'none'}}/>\n        </form>\n    )\n}\n\nexport default ItemForm","import styles from './Totals.module.css'\n\nconst Totals = (props) => {\n    const person = props.person\n\n\n    return (\n        <ul>\n            <li className={styles.totals}>\n                <span>Subtotal</span>\n           